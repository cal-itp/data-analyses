	<ns0:dataQualityInfo>
		<ns0:DQ_DataQuality>
			<ns0:scope>
				<ns0:DQ_Scope>
					<ns0:level>
						<ns0:MD_ScopeCode codeList="http://www.isotc211.org/2005/resources/Codelist/gmxCodelists.xml#MD_ScopeCode" codeListValue="feature" codeSpace="ISOTC211/19115">
							<text>feature</text>
